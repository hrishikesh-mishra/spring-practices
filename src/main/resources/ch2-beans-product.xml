<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


    <bean name="aaa" class="com.hrishikeshmishra.springpractice.ch2.shop.Battery">
        <property name="name" value="AAA" />
        <property name="price" value="12.9" />
        <property name="rechargeable" value="false" />
    </bean>

    <bean id="cdrw" class="com.hrishikeshmishra.springpractice.ch2.shop.Disc">
        <constructor-arg value="CD-RW" />
        <constructor-arg value="1.5" />
        <property name="capacity" value="700" />
    </bean>


</beans>